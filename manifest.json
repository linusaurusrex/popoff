{
	"manifest_version": 3,
	"name": "Pop Off",
	"description": "Prevent annoying chrome-less popup windows.",
	"version": "0.2.0",
	"content_scripts": [{
		"js": ["scripts/injector.js"],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}],
	"web_accessible_resources": [{
		"matches": ["<all_urls>"],
		"resources": ["scripts/open.js"],
		"use_dynamic_url": true
	}]
}
